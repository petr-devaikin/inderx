
$greyColor = #7e7e7e
$lightGreyColor = #cecece
$pinkColor = #fd4f68


transform(t)
    -ms-transform t
    -webkit-transform t
    transform t


verticalGradient(a, b)
    background: b; /* For browsers that do not support gradients */
    background: -webkit-linear-gradient(a, b); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(a, b); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(a, b); /* For Firefox 3.6 to 15 */
    background: linear-gradient(a, b); /* Standard syntax */
